{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "quotes": ["error", "single"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-shadow": "off",
    "import/prefer-default-export": 0,
    "no-unused-expressions": [
      "error", {
        "allowTernary": true
      }
    ],
    "prettier/prettier": [
      "error", {
        "semi": false,
        "singleQuote": true
      }
    ]
  }
}